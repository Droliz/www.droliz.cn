{
  "name": "www.droliz.cn",
  "private": true,
  "version": "1.0.0",
  "type": "commonjs",
  "description": "droliz's personal front-end project",
  "author": "droliz",
  "repository": {
    "type": "git",
    "url": "https://github.com/Droliz/www.droliz.cn"
  },
  "bugs": {
    "url": "http://www.drooliz.cn"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,md}": [
      "eslint --fix"
    ]
  },
  "githooks": {
    "pre-commit": "lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "lint:format": "prettier --write src/",
    "lint:style": "stylelint --fix \"src/**/*.{css,scss,vue}\"",
    "lint:check": "lint-staged",
    "prepare": "husky install",
    "commit:en": "git add . && git-cz",
    "commit:zh": "git add . && cz-customizable",
    "push": "git push"
  },
  "dependencies": {
    "alova": "^2.9.1",
    "axios": "1.2.0-alpha.1",
    "element-plus": "^2.3.7",
    "gsap": "^3.12.2",
    "mockjs": "^1.1.0",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.1.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@iconify-json/ep": "^1.1.11",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/cli-plugin-babel": "4.4.6",
    "@vue/cli-plugin-eslint": "4.4.6",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vueuse/core": "^10.2.1",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.6",
    "commitlint-config-cz": "^0.13.3",
    "commitlint-config-git-commit-emoji": "^1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^9.15.1",
    "fast-glob": "3.2.11",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.25",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.7",
    "prettier": "^2.5.1",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "scss": "^0.2.4",
    "stylelint": "^15.10.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.1.0",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-element-plus": "^0.7.1",
    "unplugin-icons": "^0.16.3",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svg-icons": "2.0.1",
    "vue-tsc": "^1.8.3"
  }
}